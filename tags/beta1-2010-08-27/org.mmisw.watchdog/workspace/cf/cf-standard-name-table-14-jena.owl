    <skos:narrower>
      <Standard_Name rdf:about="land_ice_area_fraction">
        <rdfs:comment>"X_area_fraction" means the fraction of horizontal area occupied by X. "Land ice" means glaciers, ice-caps and ice-sheets resting on bedrock and also includes ice-shelves.</rdfs:comment>
        <canonical_units>1</canonical_units>
        <rdfs:label>land_ice_area_fraction</rdfs:label>
        <skos:prefLabel>land ice area fraction</skos:prefLabel>
