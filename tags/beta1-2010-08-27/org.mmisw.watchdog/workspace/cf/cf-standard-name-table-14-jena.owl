    <skos:narrower>
      <Standard_Name rdf:about="surface_carbon_dioxide_mole_flux">
        <rdfs:comment>The surface called "surface" means the lower boundary of the atmosphere. In accordance with common usage in geophysical disciplines, "flux" implies per unit area, called "flux density" in physics.</rdfs:comment>
        <canonical_units>mol m-2 s-1</canonical_units>
        <rdfs:label>surface_carbon_dioxide_mole_flux</rdfs:label>
        <skos:prefLabel>surface carbon dioxide mole flux</skos:prefLabel>
