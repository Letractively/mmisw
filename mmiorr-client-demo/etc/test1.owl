    </DemoClass>
    <DemoClass rdf:about="http://example.org/test1/termTwo">
        <description>description of termTwo</description>
        <name>termTwo</name>
        <rdfs:label>termTwo</rdfs:label>
    </DemoClass>
    <DemoClass rdf:about="http://example.org/test1/termThree">
        <description>description of termThree</description>
        <name>termThree</name>
        <rdfs:label>termThree</rdfs:label>
    </DemoClass>
